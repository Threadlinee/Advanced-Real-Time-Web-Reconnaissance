{
    "manifest_version": 3,
    "name": "Attack Surface Explorer",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Scan Attack Surface"
    }
  }
  